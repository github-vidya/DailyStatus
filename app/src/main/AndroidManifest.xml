<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dailystatus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logolauncher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--activity registered in the application -->
        <activity android:name=".activity.Splashactivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Loginactivity"></activity>
        <activity android:name=".activity.Parentactivity"></activity>
        <activity android:name=".activity.Summaryactivity"></activity>
        <activity android:name=".activity.Studdailydataentryactivity"></activity>
        <activity android:name=".activity.NewstudentActivity"></activity>

    </application>
</manifest>
